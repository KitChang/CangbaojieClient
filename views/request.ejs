
    
      <div class="container content">
        <div class="row">
            <div class="panel panel-default full-width">
                <ol class="breadcrumb panel-heading">
                        <li><a href='/request'>我要推广</a></li>
                    </ol>  
                <div class="panel-body">
                    <button onClick="javascript: window.location.href='/request/new'" class="btn btn-default right">创建</button>
                    
                </div>
                <div class="panel-body">

                <form name="form">
                <table class="table table-striped" id="table">
                    <thead>
                    <tr><th>广告标题</th><th>省</th><th>城市</th><th>地区</th><th>街道</th><th>状态</th></tr>
                    </thead>
                    <tbody>
                <%
                var row;
                while (results.length) {
                  row = results.pop();
                %>
                    <tr><td><%=row.advertisement.title%></td>
                        <td><%=row.state%></td>
                        <td><%=row.city%></td>
                        <td><%=row.region%></td>
                        <td><%=row.street%></td>
                        <td><%=row.status%></td>
                        </tr>
                <%
                }
                %>
                    </tbody>
                </table>
                </form>
                </div>

            </div>
        </div>
        
      </div>
    


    <script>
        function destroy(rowId){
            document.form.action = "/staff/destroy/"+rowId;
            document.form.method = "delete";
            document.form.submit();
        }
    </script>
      
    <script type="text/javascript" src="/js/dependencies/jquery.min.js"></script>
    <script type="text/javascript" src="/bootstrap/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="//cdn.datatables.net/1.10.10/js/jquery.dataTables.min.js">     </script>
    <script>
        $("#table").DataTable({"language": {
                "url": "//cdn.datatables.net/plug-ins/1.10.10/i18n/Chinese.json"
            }});
    </script>

